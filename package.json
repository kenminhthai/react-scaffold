    "react-emotion": "^9.2.6",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.1.2",
    "redux": "3.6.0",
    "redux-axios-api-middleware": "0.2.0",
    "redux-thunk": "2.1.0",
    "styled-system": "^3.0.2",
    "system-components": "^3.0.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.6",
    "@storybook/addon-links": "^3.2.6",
